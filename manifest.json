{
  "name": "Amazon Smile Redirector",
  "version": "0.2",
  "description": "Forwards you to Amazon Smile anytime you hit the Amazon website",
  "icons": {
    "128": "icon.png"
  },
  "background": { 
     "page":"bg.html"
     },
   "manifest_version": 2,
   "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["content.js"]
   }
   ],
  "permissions": ["tabs"]
}
